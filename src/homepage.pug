header.header
    .container
        .header_photo
            .info
                #avadiv
                    img(src=data.bio.avatar, id="avatar")
        .header_title
            p.name #{data.bio.name}
            span.cname #{data.bio.cname}
            p.edu #{data.bio.education} at !{data.bio.affiliation}
            .icons
                a(href='mailto:aprilww@umich.edu')
                    <i class="fa fa-envelope"></i>
                a(target="_blank" href='https://twitter.com/AprilWang95')
                    <i class="fab fa-twitter"></i>
                a(target="_blank" href='https://github.com/LittleAprilFool')
                    <i class="fab fa-github"></i>
                a(target="_blank" href='https://scholar.google.com/citations?user=yitS6coAAAAJ&hl=en')
                    <i class="fa fa-graduation-cap"></i>
            .header_menu
                ul.nav
                    li#nav1
                        a(href='#/home') Home
                    li#nav2
                        a(href='#/cv') CV
                    li#nav3
                        a(href='#publications') Publications
main
    .home
        section.about
            .container
                .news.panel
                    p.title Attending
                    .deco
